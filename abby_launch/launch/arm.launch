<launch>
    <arg name="irc5_ip" default="192.168.0.50" />
    <node name="irc5_sender" pkg="irc5" type="motion_download_interface" args="$(arg irc5_ip)" />
    <node name="irc5_receiver" pkg="irc5" type="robot_state" args="$(arg irc5_ip)" />
    <include file="$(find irb_120_arm_navigation)/launch/environment.launch" />
</launch>
